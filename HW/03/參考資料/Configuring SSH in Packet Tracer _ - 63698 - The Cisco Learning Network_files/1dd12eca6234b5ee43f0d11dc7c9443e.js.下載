jive.namespace('nitro');

jive.nitro.Nitro = jive.oo.Class.extend(function(protect, _super) {

    this.init = function(options) {
        this.options = options || {};
        this.methods = [];
        this.dfd = new $j.Deferred();
        
        this.dfd.done(this.setSession);
        
        if (this.options.hasOwnProperty('server')) {
            this.dfd.resolveWith(this, [this.options]);
        } else {
            this.loadSession();
        }
    };
    
    this.execute = function(callback) {
        var promise = new jive.conc.Promise();
        var self = this;
        
        this.dfd.done(function() {
            var methodString = this.generateAllMethodsString();
            
            $j.getJSON(self.serverUrl + escape(methodString), function(res) {
                $j.extend(res, {
                    eachMethod: function(methodName, callback) {
                        self.eachMethod(res, methodName, callback);
                    }
                });
                self.handleNitroResponse(res, promise, callback);
            });
        });
        
        return promise;
    };
    
    this.addMethod = function(name, params) {
        this.methods.push(this.createMethodString(name, params));
        
        return this;
    };

    this.isLocalizationEnabled = function() {
        return this.localizationEnabled || false;
    };

    protect.eachMethod = function(res, methodName, callback) {
        $j.each(res.Nitro.Nitro, function(i, method) {
            if (method.method === methodName) {
                callback.call(this, method);
            }
        });
    };
    
    protect.generateAllMethodsString = function() {
        this.methods.unshift(this.createMethodString('user.login', this.loginParameters));
        
        var methodString = '[' + this.methods.join(',') + ']';
        
        this.methods = [];
        
        return methodString;
    };
    
    protect.setSession = function(data) {
        this.serverUrl = (data.server || data.baseUrl) + '?jsCallback=?&method=batch.run&methodFeed=';
        this.localizationEnabled = data.localizationEnabled || false;
        this.loginParameters = {
            userId: data.userID,
            apiKey:  data.apiKey,
            ts: data.timeStamp,
            sig: data.signature
        };
    };
    
    protect.loadSession = function() {
        var self = this;
        
        $j.ajax({
            url: jive.rest.url("/nitro/admin/session"),
            success: function(response) {
                self.dfd.resolveWith(self, [response]);
            },
            error: function(jqXHR, textStatus, errorThrown) {
                console.log('Error getting session: '+textStatus+'/'+errorThrown);
                self.dfd.rejectWith(self, [textStatus, errorThrown]);
            }
        });
    };
    
    protect.handleNitroResponse = function(res, promise, callback) {
        var nitro = res.Nitro;
        if (typeof nitro.Error != "undefined") {
            promise.emitError(nitro.Error.Code, nitro.Error.Message);
        } else {
            var noErrors = true;
            $j.each(nitro.Nitro, function(i, method) {
                if (typeof method.Error != "undefined") {
                    promise.emitError(method.Error.Code, method.Error.Message);
                    noErrors = false;
                }
            });
            
            if (noErrors) {
                callback.call(this, res, promise);
            }
        }
    };
    
    protect.createMethodString = function(name, params) {
        var result = '"method='+name;
        
        $j.each(params, function(key, value) {
            result += '&'+key+'='+value;
        });
        
        result += '"';
        
        return result;
    };

});

;
jive.namespace('nitro.shared');

jive.nitro.shared.UserService = jive.RestService.extend(function(protect, _super) {
    
    this.init = function(options) {
        _super.init.call(this, options);
        this.options = options;
    };
    
    this.getProfileInfo = function(options) {
        var success = options.success;
        var error = options.error;
        
        delete options.success;
        delete options.error;
        
        $j.ajax({
            url: jive.rest.url("/nitro/users/data"),
            data: options,
            success: success,
            error: error
        });
    };
    
    this.getUserIDs = function(jiveUserIDs) {
        var dfd = new $j.Deferred();
        var self = this;
        
        $j.ajax({
            url: jive.rest.url("/nitro/users/ids"),
            data: {
                userId: jiveUserIDs
            },
            success: function(response) {
                dfd.resolveWith(self, [response]);
            },
            error: function(jqXHR, textStatus, errorThrown) {
                console.log('Error getting userIds: '+textStatus+'/'+errorThrown);
                dfd.rejectWith(self, [textStatus, errorThrown]);
            }
        });
        
        return dfd;
    };
    
});

;
jive.namespace('cisco.acclaim');

jive.cisco.acclaim.AcclaimService = jive.RestService.extend(function (protect, _super) {

  this.init = function (options) {
    _super.init.call(this, options);
    this.options = options;
  };

  this.isAcclaimUserEnabled = function (userID) {
    var deferred = $j.Deferred();

    $j.ajax({
      type: 'POST',
      contentType: 'application/json',
      url: jive.rest.url('/ciscoAcclaim/enabled') + '?u=' + userID,
      success: function (enabled) {
        //console.log("isAcclaimUserEnabled response=[" + (enabled || "null") + "]");
        deferred.resolveWith(enabled);
      },
      error: function (jqXHR, textStatus, errorThrown) {
        //console.log("Hover badges request failed [" + JSON.stringify(jqXHR) + "] and [" + textStatus + "] and [" + errorThrown + "]");
        deferred.resolveWith(false);
      }
    });

    return deferred;
  };

  this.getAcclaimAvatarBadges = function (userIDs) {
    var deferred = $j.Deferred();

    /*var us = "";
     $j(userIDs).each(function(i, uid) {
     us += "us=" + uid + "&";
     });
     us = us.substring(0, us.length-1);*/
    $j.ajax({
      type: 'POST',
      contentType: 'application/json',
      url: jive.rest.url("/ciscoAcclaim/avatars"),
      dataType: 'json',
      data: JSON.stringify(userIDs),
      success: function (avatarBadges) {
      console.log('avatar badges only ' + JSON.stringify(avatarBadges));
        deferred.resolveWith(avatarBadges);
      },
      error: function (jqXHR, textStatus, errorThrown) {
        console.log("Failed to fetch Acclaim Avatar Badges with response=[" + jqXHR + "] and status=[" + textStatus + "] and error=[" + errorThrown + "]");
        deferred.resolveWith([]);
      }
    });

    return deferred;
  };

  this.getAcclaimProfileBadges = function (userID) {
    //console.log("getAcclaimProfileBadges(" + userID + ")");
    var deferred = $j.Deferred();

    $j.ajax({
      type: 'POST',
      contentType: 'application/json',
      url: jive.rest.url('/ciscoAcclaim') + '?u=' + userID,
      success: function (userBadges) {
        //console.log("getAcclaimProfileBadges response=[" + (userBadges || "null") + "]");
        deferred.resolveWith(userBadges);
      },
      error: function (jqXHR, textStatus, errorThrown) {
        //console.log("Hover badges request failed [" + JSON.stringify(jqXHR) + "] and [" + textStatus + "] and [" + errorThrown + "]");
        deferred.resolveWith([]);
      }
    });

    return deferred;
  };

  this.getAcclaimUserProfile = function (userID) {
    var deferred = $j.Deferred();

    $j.ajax({
      type: 'POST',
      contentType: 'application/json',
      url: jive.rest.url('/ciscoAcclaim/with') + '?u=' + userID,
      success: function (userBadges) {
        //console.log("getAcclaimProfileBadges response=[" + (userBadges || "null") + "]");
        deferred.resolveWith(userBadges);
      },
      error: function (jqXHR, textStatus, errorThrown) {
        //console.log("Hover badges request failed [" + JSON.stringify(jqXHR) + "] and [" + textStatus + "] and [" + errorThrown + "]");
        deferred.resolveWith([]);
      }
    });

    return deferred;
  };

  this.updateMyCertifications = function (ids, userID) {
    console.log('jive.cisco.acclaim.AcclaimService.updateMyCertifications(' + ids + ')');
    var deferred = $j.Deferred();

    $j.ajax({
      type: 'POST',
      contentType: 'application/json',
      url: jive.rest.url('/ciscoAcclaim/my') + '?updatedCerts=' + ids + '&u=' + userID,
      success: function (result) {
        console.log('jive.cisco.acclaim.AcclaimService.updateMyCertifications()->success');
      },
      error: function (jqXHR, textStatus, errorThrown) {
        console.log("Update My Certifications request failed [" + JSON.stringify(jqXHR) + "] and [" + textStatus + "] and [" + errorThrown + "]");
      }
    })
  }

});

;
jive.namespace('nitro.content');

/**
 * @depends path=/plugins/gamification/resources/script/jive-nitro.js
 * @depends path=/plugins/gamification/resources/script/apps/shared/models/userService.js
 * @depends path=/plugins/gamification/resources/script/apps/shared/models/acclaimService.js
 */
jive.nitro.content.ContentSource = jive.RestService.extend(function(protect, _super) {
    
    this.init = function(options) {
        _super.init.call(this, options);
        this.options = options;
    };
    
    this.loadStatusLevels = function(jiveUserIDs) {
        var promise = new jive.conc.Promise();
        var userService = new jive.nitro.shared.UserService({});
        var self = this;
        
        userService.getUserIDs(jiveUserIDs).done(function(userIDs) {
            self.loadStatusFromNitro(userIDs, promise);
        });
        
        return promise;
    };
    
    protect.loadStatusFromNitro = function(userIDs, promise) {
        var self = this;
        var nitro = new jive.nitro.Nitro(this.options);
        var locale = n4jive.locale(this.options.localizationEnabled);

        nitro.addMethod('user.getLevel', n4jive.extendWithLocale({
            userIds: Object.values(userIDs).join(',')
        },locale));
        
        nitro.execute(function(res) {
            self.handleNitroResponse(res, userIDs, promise);
        });
    };
    
    this.handleNitroResponse = function(res, userIDs, promise) {
        var nitro = res.Nitro;
        
        $j.each(nitro.Nitro, function(i, method) {
            if (method.method === "user.getLevel") {
                var users = {};
                
                $j.each($j.makeArray(method.users.User), function(j, user) {
                    users[user.userId] = user.SiteLevel;
                });
                
                $j.each(userIDs, function(jiveUserID, userID) {
                    users[userID].jiveUserID = jiveUserID;
                }); 
                
                promise.emitSuccess(users);
            }
        });
    };
    
    this.loadSpecialBadges = function(jiveUserIDs) {
        var promise = new jive.conc.Promise();
        var userService = new jive.nitro.shared.UserService({});
        var self = this;
        
        userService.getUserIDs(jiveUserIDs).done(function(userIDs) {
            self.loadBadgesFromNitro(userIDs, promise);
        });

        return promise;
    };

    this.loadAcclaimBadges = function (jiveUserIDs) {
        var promise = new jive.conc.Promise();
        var acclaimService = new jive.cisco.acclaim.AcclaimService({});

        acclaimService.getAcclaimAvatarBadges(jiveUserIDs).done(function (avatarBadges) {
            promise.emitSuccess(this);   // Why is 'this' the data and not 'avatarBadges'?
        });

        return promise;
    };
   
    protect.loadBadgesFromNitro = function(userIDs, promise) {
        var self = this;
        var nitro = new jive.nitro.Nitro(this.options);
        var locale = n4jive.locale(this.options.localizationEnabled);

        self.addUserChallengeMethods(userIDs, nitro, locale);
        
        nitro.execute(function(res) {
            self.handleNitroChallengeResponse(res, userIDs, promise);
        });
    };
    

    protect.addUserChallengeMethods = function(userIDs, nitro, locale) {
		$j.each(userIDs, function(userID, index) {
			nitro.addMethod('user.getChallengeProgress', n4jive
					.extendWithLocale({
						userId : userID,
						showOnlyTrophies : true,
						folder : "Cisco" // only return badges in Cisco folder to limit results
					}, locale));
		});
	};
	
    this.handleNitroChallengeResponse = function(res, userIDs, promise) {
    	var self = this;
        var nitro = res.Nitro;

        var userChallenges = {};
        var challengeIndex = 0;
        // assumes that nitro method responses are in same order as userids being requested
        $j.each(nitro.Nitro, function(i, method) {
            if (method.method === "user.getChallengeProgress") {
            	userChallenges[challengeIndex] = {};
            	$j.each($j.makeArray(method.challenges.Challenge), function(j, challenge) {
            		if(self.shouldSetChallenge(challenge, userChallenges[challengeIndex])) {
            			userChallenges[challengeIndex].iconUrl = challenge.fullUrl;
            			userChallenges[challengeIndex].name = challenge.name;
            		}
            	});
            	challengeIndex++;
            }
        });
        
        var index = 0;
        $j.each(userIDs, function(key, value) {
        	userChallenges[index++].jiveUserID = value;
        })
        
        promise.emitSuccess(userChallenges);
    };
    
    protect.shouldSetChallenge = function(challenge, userChallenge) {
    	switch(challenge.name) {
    	case "Cisco Subject Matter Expert":
    		return true;
    	case "Cisco Employee":
    		return userChallenge == undefined || userChallenge.name != "Cisco Subject Matter Expert";
    	case "VIP Member":
    		return userChallenge == undefined || (userChallenge.name != "Cisco Subject Matter Expert" && userChallenge.name != "Cisco Employee");
    	case "Community Spotlight":
    		return userChallenge == undefined || (userChallenge.name != "Cisco Subject Matter Expert" && userChallenge.name != "Cisco Employee" && userChallenge.name != "VIP Member");
		default:
			return false;
    	}
    }
    
});

;
// This file was automatically generated from content.soy.
// Please don't edit this file by hand.

goog.provide('jive.ps.nitro.content.acclaimBadge');
goog.provide('jive.ps.nitro.content.head');
goog.provide('jive.ps.nitro.content.imageTag');
goog.provide('jive.ps.nitro.content.statusLevelImage');

goog.require('soy');
goog.require('soydata');
goog.require('soy.StringBuilder');
goog.require('jive.shared.soy.resourceInlineJs');


jive.ps.nitro.content.statusLevelImage = function(opt_data, opt_sb) {
  var output = opt_sb || new soy.StringBuilder();
  output.append('<span class="j-status-levels j-gamification-status-level">');
  jive.ps.nitro.content.imageTag(opt_data, output);
  output.append('</span>');
  return opt_sb ? '' : output.toString();
};


jive.ps.nitro.content.imageTag = function(opt_data, opt_sb) {
  var output = opt_sb || new soy.StringBuilder();
  output.append('<img src="', soy.$$escapeHtml(opt_data.imagePath), '" alt="', soy.$$escapeHtml(opt_data.title), '" title="', soy.$$escapeHtml(opt_data.title), '"  ', (opt_data.badge) ? 'class="cisco-special-badge"' : '', ' />');
  return opt_sb ? '' : output.toString();
};


jive.ps.nitro.content.acclaimBadge = function(opt_data, opt_sb) {
  var output = opt_sb || new soy.StringBuilder();
  output.append('<img src="', soy.$$escapeHtml(opt_data.badgeURL), '" id="', soy.$$escapeHtml(opt_data.badgeID), '" alt="', soy.$$escapeHtml(opt_data.badgeName), '" title="', soy.$$escapeHtml(opt_data.badgeName), '" class="cisco-acclaim-badge-avatar"/>');
  return opt_sb ? '' : output.toString();
};


jive.ps.nitro.content.head = function(opt_data, opt_sb) {
  var output = opt_sb || new soy.StringBuilder();
  jive.shared.soy.resourceInlineJs({code: '$j(document).ready(function() {new jive.nitro.content.Main({apiKey: "' + soy.$$escapeHtml(opt_data.apiKey) + '", timeStamp: "' + soy.$$escapeHtml(opt_data.signatureTimestamp) + '", signature: "' + soy.$$escapeHtml(opt_data.signatureValue) + '", server: "' + soy.$$escapeHtml(opt_data.baseUrl) + '", userID: "' + soy.$$escapeHtml(opt_data.userID) + '", localizationEnabled: ' + soy.$$escapeHtml(opt_data.localizationEnabled) + ', ciscoAcclaimSystemEnabled: ' + soy.$$escapeHtml(opt_data.ciscoAcclaimSystemEnabled) + '});});'}, output);
  return opt_sb ? '' : output.toString();
};

;
jive.namespace('nitro.content');

/**
 * @depends template=jive.ps.nitro.content.statusLevelImage
 * @depends template=jive.ps.nitro.content.acclaimBadge
 */
jive.nitro.content.ContentView = jive.AbstractView.extend(function(protect, _super) {
    
    jive.conc.observable(this);
    
    this.init = function(options) {
        this.options = options;
        var view = this;
        
        $j(function () {
            view.emit('viewReady');
        });
        
    };
    
    this.showStatusLevels = function(users) {
      var view = this;
        $j(".jive-comment a.j-avatar, div.j-post-avatar a.j-avatar").each(function(i, link) {
          var $avatar = $j(link);
          var jiveUserID = $avatar.data('userid');

            $j.each(users, function(userID, user) {
                if (jiveUserID == user.jiveUserID) {
                    $j(link).append(jive.ps.nitro.content.statusLevelImage({
                        imagePath: user.iconUrl,
                        title: user.name
                    }));
                }
              view.reorderBadges($avatar);
            });
        });
    };

    this.showBadges = function(userChallenges) {
      var view = this;
    	$j(".jive-comment a.j-avatar, div.j-post-avatar a.j-avatar").each(function(i, link) {
        var $avatar = $j(link);
        var jiveUserID = $avatar.data('userid');
        var spanElement = $avatar.find(".j-gamification-status-level").last();
    		
    		$j.each(userChallenges, function(userID, challenge) {
    			if (jiveUserID == challenge.jiveUserID && challenge.iconUrl != undefined) {
    				$j(spanElement).append(jive.ps.nitro.content.imageTag({
    					imagePath: challenge.iconUrl,
    					title: challenge.name,
    					badge: true
    				}));
    			}
    		});

        view.reorderBadges($avatar);
    	});
    };

  this.showAcclaimBadges = function (userAcclaimBadges) {
    var view = this;
    if (userAcclaimBadges) {
      $j(".jive-comment a.j-avatar, div.j-post-avatar a.j-avatar").each(function (i, avatar) {
        var $avatar = $j(avatar);
        var jiveUserID = $avatar.data('userid');
        $j(userAcclaimBadges).each(function (i, avatarBadge) {
          if (jiveUserID == avatarBadge.userID) {
            var acclaimBadgeHTML = jive.ps.nitro.content.acclaimBadge({
            	badgeID: avatarBadge.badgeID,
            	badgeName: avatarBadge.name,
            	badgeURL: avatarBadge.badgeURL
            });
            var $badgeContainer = $avatar.find('span.j-gamification-status-level').last();
            if (!$badgeContainer.length) {
              $badgeContainer = $j('<span></span>').addClass('j-status-levels').addClass('j-gamification-status-level').addClass('cisco-acclaim-badge-container');
              $avatar.append($badgeContainer);
            }
            $badgeContainer.append(acclaimBadgeHTML);
            view.reorderBadges($avatar);
          }
        });
      });
    }
  };
    
    this.getUserIDs = function() {
        var userIDs = [];
        
        $j("a.j-avatar, div.j-post-avatar a.j-avatar").each(function(i, link) {
            var userID = $j(link).data('userid');
            if ($j.inArray(userID, userIDs) === -1) {
                userIDs.push(userID);
            }
        });
        return userIDs;
    };

  protect.reorderBadges = function ($avatar) {
    var $certification = $avatar.find('img.cisco-acclaim-badge-avatar');
    var $containers = $avatar.find('span.j-gamification-status-level');
    if ($certification.length && $containers.length) {
      var $statusContainer, $certificationContainer;
      $containers.each(function (i, container) {
        var $container = $j(container);
        if ($container.hasClass('cisco-acclaim-badge-container')) {
          $certificationContainer = $container;
        } else {
          $statusContainer = $container;
        }
      });
      if ($statusContainer && $certificationContainer) {
        $certification.detach();
        $statusContainer.find('img:nth-child(1)').after($certification);
        $certificationContainer.remove();
      }
    }
  }
});

;
jive.namespace('nitro.content');

/**
 * @depends path=/plugins/gamification/resources/script/apps/content/models/content_source.js
 * @depends path=/plugins/gamification/resources/script/apps/content/views/content_view.js
 */
jive.nitro.content.Main = jive.oo.Class.extend(function() {

    this.init = function(options) {
        this.options = options;
        this.source = new jive.nitro.content.ContentSource(options);
        this.view = new jive.nitro.content.ContentView(options);
        
        var main = this;
        
        jive.rte.renderedContent.addListener("renderedContent", function(container) {
            var id = $j(container).attr('id');
            if (container && ("jive-comments" === id || "jive-thread-messages-container" == id)) {
                main.loadStatusLevels();
            	main.loadSpecialBadges();
            }
        });
        
        this.view.addListener('viewReady', $j.proxy(this.loadStatusLevels, this));
        this.view.addListener('viewReady', $j.proxy(this.loadSpecialBadges, this));
      if (this.options.ciscoAcclaimSystemEnabled) {
        this.view.addListener('viewReady', $j.proxy(this.loadAcclaimBadges, this));
      }
    };
    
    this.loadStatusLevels = function() {
        var main = this;
        main.source.loadStatusLevels(main.view.getUserIDs()).addCallback(function(users) {
            main.view.showStatusLevels(users);
            jive.localexchange.emit("nitro.content.userStatusLoaded");
        });
    };

    this.loadSpecialBadges = function() {
    	var main = this;

      main.source.loadSpecialBadges(main.view.getUserIDs()).addCallback(function (users) {
        main.view.showBadges(users);
        jive.localexchange.emit("nitro.content.userBadgesLoaded");
      });
    };

  this.loadAcclaimBadges = function () {
    var main = this;

    var userIDs = main.view.getUserIDs();
    console.log("Requesting Acclaim Avatar Badges for users [" + userIDs + "]");
    main.source.loadAcclaimBadges(userIDs).addCallback(function (badges) {
      main.view.showAcclaimBadges(badges);
      jive.localexchange.emit("cisco.acclaim.acclaimBadgesLoaded");
    });
  }

});

;
