jive.CollapseTableRows = jive.oo.Class.extend(function(protect) {
	this.init = function(id) {
		var self = this;
		self.elementID = id;
		console.debug('init:' + self.elementID);
	};
	
	this.hideRows = function(rowElement) {
		var self = this;
		//console.log('hide:' + self.elementID);
		rowElement.removeClass("collapsetablerows_opened");
		rowElement.addClass("collapsetablerows_closed");
		var numberOfRowsToCollapse = rowElement.attr("numberofrowstocollapse");
		var parentRow = rowElement.parents("tr");
		parentRow.nextUntil('', ':lt(' + numberOfRowsToCollapse + ')').hide();
	};
	
	this.showRows = function(rowElement) {
		var self = this;
		//console.log('show:' + self.elementID);
		rowElement.addClass("collapsetablerows_opened");
		rowElement.removeClass("collapsetablerows_closed");
		var numberOfRowsToShow = rowElement.attr("numberofrowstocollapse");
		var parentRow = rowElement.parents("tr");
		parentRow.nextUntil('', ':lt(' + numberOfRowsToShow + ')').show();
	};
	
	this.toggleRows = function() {
		var self = this;
		//console.log('toggle:' + self.elementID);
		$j("#"+ self.elementID).each(function() { 
			var rowElement = $j(this);
			$j(rowElement).each(function() {
				  $j.each(this.attributes, function() {
				    // this.attributes is not a plain object, but an array
				    // of attribute nodes, which contain both the name and value
				    if(this.specified) {
				      console.log(this.name, this.value);
				    }
				  });
			});
			if(rowElement.hasClass("collapsetablerows_opened")) {
				self.hideRows(rowElement);
				rowElement.text(rowElement.attr("expandtext") || "Show Titles");
			} else {
				self.showRows(rowElement);
				rowElement.text(rowElement.attr("collapsetext") || "Hide Titles");
			}
		});
	};
});
